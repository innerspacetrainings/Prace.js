root = true

[*]
end_of_line              = lf
charset                  = utf-8
indent_style             = tab
insert_final_newline     = true
trim_trailing_whitespace = true

[*.xml]
indent_style = space

[*.md]
trim_trailing_whitespace = false
indent_style             = space

[*.yml]
indent_style = space
indent_size = 4